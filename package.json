{
  "name": "@onivoro/server-process",
  "version": "1.17.0",
  "repository": {
    "url": "https://github.com/onivoro/server-process.git"
  },
  "exports": {
    "import": "./mjs/index.js",
    "require": "./cjs/index.js"
  },
  "type": "module",
  "scripts": {
    "test": "jest",
    "build": "tsc -p tsconfig.json && rollup -c rollup.config.js",
